{
  "name": "@virtualidentityag/component--library",
  "private": true,
  "version": "0.0.1",
  "description": "VI Starter for a Stencil based Web Components Library",
  "scripts": {
    "init": "node init.js",
    "build": "stencil build && npm run storybook:build",
    "build:watch": "stencil build --watch && npm run storybook:build",
    "build:es5": "stencil build --es5 && npm run storybook:build",
    "start": "npm run build --config ./stencil-readme.config.ts && npm run storybook:watch",
    "start:es5": "stencil build --dev --watch --serve --es5",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "npm run generate:component",
    "generate:component": "node component.js",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:fix": "npm run lint:code:fix && npm run lint:style:fix",
    "lint:code": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" \"./*.js\"",
    "lint:code:fix": "eslint \"./src/**/*.ts\" \"./src/**/*.tsx\" \"./*.js\" --fix",
    "lint:style": "stylelint \"./src/**/*.css\" \"./src/**/*.scss\"",
    "lint:style:fix": "stylelint \"./src/**/*.css\" \"./src/**/*.scss\" --fix",
    "docs": "typedoc src/components/ui-*",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "storybook:build": "build-storybook",
    "storybook:watch": "stencil build --watch & start-storybook -p 6006 -s ./dist,./www"
  },
  "devDependencies": {
    "@babel/core": "7.10.5",
    "@biotope/quality-gate": "git+ssh://git@github.com/biotope/biotope-quality-gate.git#task/repurpose-package",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.1",
    "@semantic-release/git": "9.0.0",
    "@stencil/core": "1.17.2",
    "@stencil/router": "1.0.1",
    "@stencil/sass": "1.3.2",
    "@storybook/addon-a11y": "6.0.0-rc.18",
    "@storybook/addon-actions": "6.0.0-rc.18",
    "@storybook/addon-knobs": "6.0.0-rc.18",
    "@storybook/addon-notes": "5.3.19",
    "@storybook/addon-storysource": "6.0.0-rc.18",
    "@storybook/addon-viewport": "6.0.0-rc.18",
    "@storybook/addons": "6.0.0-rc.18",
    "@storybook/cli": "6.0.0-rc.18",
    "@storybook/html": "6.0.0-rc.18",
    "@storybook/theming": "6.0.0-rc.18",
    "@types/jest": "24.9.1",
    "@types/puppeteer": "2.0.1",
    "babel-loader": "8.1.0",
    "cz-conventional-changelog": "3.2.0",
    "faker": "^4.1.0",
    "fs-extra": "9.0.1",
    "glob": "7.1.6",
    "husky": "4.2.5",
    "inquirer": "^7.3.2",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "prettier-eslint-cli": "5.0.0",
    "prompt-confirm": "2.0.4",
    "puppeteer": "2.1.1",
    "replace": "^1.2.0",
    "semantic-release": "17.1.1",
    "ts-loader": "8.0.1",
    "typedoc": "0.17.8",
    "typescript": "3.9.6",
    "workbox-build": "4.3.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release": {
    "branches": [
      "master-v4"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {}
}
